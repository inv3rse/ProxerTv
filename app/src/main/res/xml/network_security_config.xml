<?xml version="1.0" encoding="utf-8"?>
<network-security-config>

    <!-- allow http traffic -->
    <base-config cleartextTrafficPermitted="true" />

    <debug-overrides>
        <trust-anchors>
            <certificates src="system" />
            <certificates src="user" />

            <certificates src="@raw/dfoelster_desktop" />
        </trust-anchors>
    </debug-overrides>
</network-security-config>